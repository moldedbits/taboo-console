<h1>Upload New Word List</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3">
    <%= form_tag '/taboo_word_lists/',multipart: true,method: :post do %>
      <p>
        <%= file_field_tag 'upload',accept: 'text/csv' %>
      </p>
      <p>
        <%= submit_tag "Upload",data: {confirm: "Are you sure?"} %>
      </p>
    <% end %>

	</div>
</div>
